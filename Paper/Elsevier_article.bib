@article{Crawford2015,
abstract = {The total energy storage system cost is determined by means of a robust performance-based cost model for multiple flow battery chemistries. Systems aspects such as shunt current losses, pumping losses and various flow patterns through electrodes are accounted for. The system cost minimizing objective function determines stack design by optimizing the state of charge operating range, along with current density and current-normalized flow. The model cost estimates are validated using 2-kW stack performance data for the same size electrodes and operating conditions. Using our validated tool, it has been demonstrated that an optimized all-vanadium system has an estimated system cost of {\textless} {\$}350 kWh{\textless}sup{\textgreater}-1{\textless}/sup{\textgreater} for 4-h application. With an anticipated decrease in component costs facilitated by economies of scale from larger production volumes, coupled with performance improvements enabled by technology development, the system cost is expected to decrease to 160 kWh{\textless}sup{\textgreater}-1{\textless}/sup{\textgreater} for a 4-h application, and to {\$}100 kWh{\textless}sup{\textgreater}-1{\textless}/sup{\textgreater} for a 10-h application. This tool has been shared with the redox flow battery community to enable cost estimation using their stack data and guide future direction.},
author = {Crawford, Alasdair and Viswanathan, Vilayanur and Stephenson, David and Wang, Wei and Thomsen, Edwin and Reed, David and Li, Bin and Balducci, Patrick and Kintner-Meyer, Michael and Sprenkle, Vincent},
doi = {10.1016/j.jpowsour.2015.05.066},
file = {:C$\backslash$:/Users/cdtgroup/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Crawford et al. - 2015 - Comparative analysis for various redox flow batteries chemistries using a cost performance model.pdf:pdf},
isbn = {0378-7753},
issn = {03787753},
journal = {Journal of Power Sources},
keywords = {Cost,Energy,Flow,Interdigitated,Performance,Power},
